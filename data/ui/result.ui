<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <requires lib="gtk" version="4.0"/>
    <requires lib="libadwaita" version="1.5"/>

    <object class="GtkBox" id="result_page">
        <property name="orientation">vertical</property>
        <property name="halign">center</property>
        <property name="valign">center</property>

        <property name="margin-top">32</property>
        <property name="margin-bottom">32</property>
        <property name="margin-start">24</property>
        <property name="margin-end">24</property>
        <property name="spacing">24</property>

        <child>
            <object class="GtkBox" id="result_content_box">
                <property name="orientation">vertical</property>
                <property name="spacing">24</property>

                <child>
                    <object class="GtkBox">
                        <property name="orientation">horizontal</property>
                        <property name="halign">center</property>
                        <property name="spacing">8</property>
                        <property name="margin-bottom">0</property>
                        <style>
                            <class name="iq-frame"/>
                        </style>
                        <child>
                            <object class="GtkLabel" id="final_iq_label">
                                <property name="label">0</property>
                                <property name="valign">center</property>
                                <style>
                                    <class name="title-1"/>
                                </style>
                            </object>
                        </child>
                        <child>
                            <object class="GtkLabel" id="iq_subtitle_label">
                                <property name="label">IQ</property>
                                <property name="valign">center</property>
                                <property name="margin-top">4</property>
                                <style>
                                    <class name="dim-label"/>
                                    <class name="title-4"/>
                                </style>
                            </object>
                        </child>
                    </object>
                </child>

                <child>
                    <object class="GtkBox">
                        <property name="orientation">horizontal</property>
                        <property name="spacing">24</property>

                        <property name="homogeneous">True</property>

                        <child>
                            <object class="GtkBox">
                                <property name="orientation">vertical</property>
                                <property name="spacing">18</property>
                                

                                <child>
                                    <object class="AdwPreferencesGroup" id="result_header_group">
                                        <child>
                                            <object class="AdwActionRow" id="diagnosis_row"/>
                                        </child>
                                    </object>
                                </child>

                                <child>
                                    <object class="AdwPreferencesGroup" id="stats_group">
                                        <child>
                                            <object class="AdwActionRow" id="total_score_row">
                                                <child type="suffix">
                                                    <object class="GtkLabel" id="total_score_value">
                                                        <style><class name="dim-label"/></style>
                                                    </object>
                                                </child>
                                            </object>
                                        </child>
                                        <child>
                                            <object class="AdwActionRow" id="time_taken_row">
                                                <child type="suffix">
                                                    <object class="GtkLabel" id="time_taken_value">
                                                        <style><class name="dim-label"/></style>
                                                    </object>
                                                </child>
                                            </object>
                                        </child>
                                        <child>
                                            <object class="AdwActionRow" id="age_row">
                                                <child type="suffix">
                                                    <object class="GtkLabel" id="age_value">
                                                        <style><class name="dim-label"/></style>
                                                    </object>
                                                </child>
                                            </object>
                                        </child>
                                    </object>
                                </child>

                            </object>
                        </child>

                        <child>
                            <object class="GtkBox">
                                <property name="orientation">vertical</property>
                                <property name="spacing">18</property>
                                
                                <child>
                                    <object class="AdwPreferencesGroup" id="series_group">
                                        <child>
                                            <object class="AdwActionRow" id="series_header_row">
                                                <property name="activatable">False</property>
                                                <property name="sensitive">False</property>
                                                <child type="suffix">
                                                    <object class="GtkBox">
                                                        <property name="spacing">24</property>
                                                        <child>
                                                            <object class="GtkLabel" id="series_header_correct">
                                                                <property name="label" translatable="yes">Correct</property>
                                                                <property name="width-request">50</property>
                                                                <property name="halign">center</property>
                                                                
                                                                <style><class name="caption-heading"/></style>
                                                            </object>
                                                        </child>
                                                        <child>
                                                            <object class="GtkLabel" id="series_header_deviation">
                                                                <property name="label" translatable="yes">Deviation</property>
                                                                <property name="width-request">60</property>
                                                                <property name="halign">center</property>
                                                                
                                                                <style><class name="caption-heading"/></style>
                                                            </object>
                                                        </child>
                                                    </object>
                                                </child>
                                            </object>
                                        </child>
                                        <child>
                                            <object class="AdwActionRow" id="series_a_row">
                                                <child type="suffix">
                                                    <object class="GtkBox">
                                                        <property name="spacing">24</property>
                                                        <child>
                                                            <object class="GtkLabel" id="series_a_score">
                                                                <property name="width-request">50</property>
                                                                <property name="halign">center</property>
                                                                
                                                                <style><class name="dim-label"/></style>
                                                            </object>
                                                        </child>
                                                        <child>
                                                            <object class="GtkLabel" id="series_a_deviation">
                                                                <property name="width-request">60</property>
                                                                <property name="halign">center</property>
                                                                
                                                            </object>
                                                        </child>
                                                    </object>
                                                </child>
                                            </object>
                                        </child>
                                        <child>
                                            <object class="AdwActionRow" id="series_b_row">
                                                <child type="suffix">
                                                    <object class="GtkBox">
                                                        <property name="spacing">24</property>
                                                        <child>
                                                            <object class="GtkLabel" id="series_b_score">
                                                                <property name="width-request">50</property>
                                                                <property name="halign">center</property>
                                                                
                                                                <style><class name="dim-label"/></style>
                                                            </object>
                                                        </child>
                                                        <child>
                                                            <object class="GtkLabel" id="series_b_deviation">
                                                                <property name="width-request">60</property>
                                                                <property name="halign">center</property>
                                                                
                                                            </object>
                                                        </child>
                                                    </object>
                                                </child>
                                            </object>
                                        </child>
                                        <child>
                                            <object class="AdwActionRow" id="series_c_row">
                                                <child type="suffix">
                                                    <object class="GtkBox">
                                                        <property name="spacing">24</property>
                                                        <child>
                                                            <object class="GtkLabel" id="series_c_score">
                                                                <property name="width-request">50</property>
                                                                <property name="halign">center</property>
                                                                
                                                                <style><class name="dim-label"/></style>
                                                            </object>
                                                        </child>
                                                        <child>
                                                            <object class="GtkLabel" id="series_c_deviation">
                                                                <property name="width-request">60</property>
                                                                <property name="halign">center</property>
                                                                
                                                            </object>
                                                        </child>
                                                    </object>
                                                </child>
                                            </object>
                                        </child>
                                        <child>
                                            <object class="AdwActionRow" id="series_d_row">
                                                <child type="suffix">
                                                    <object class="GtkBox">
                                                        <property name="spacing">24</property>
                                                        <child>
                                                            <object class="GtkLabel" id="series_d_score">
                                                                <property name="width-request">50</property>
                                                                <property name="halign">center</property>
                                                                
                                                                <style><class name="dim-label"/></style>
                                                            </object>
                                                        </child>
                                                        <child>
                                                            <object class="GtkLabel" id="series_d_deviation">
                                                                <property name="width-request">60</property>
                                                                <property name="halign">center</property>
                                                                
                                                            </object>
                                                        </child>
                                                    </object>
                                                </child>
                                            </object>
                                        </child>
                                        <child>
                                            <object class="AdwActionRow" id="series_e_row">
                                                <child type="suffix">
                                                    <object class="GtkBox">
                                                        <property name="spacing">24</property>
                                                        <child>
                                                            <object class="GtkLabel" id="series_e_score">
                                                                <property name="width-request">50</property>
                                                                <property name="halign">center</property>
                                                                
                                                                <style><class name="dim-label"/></style>
                                                            </object>
                                                        </child>
                                                        <child>
                                                            <object class="GtkLabel" id="series_e_deviation">
                                                                <property name="width-request">60</property>
                                                                <property name="halign">center</property>
                                                                
                                                            </object>
                                                        </child>
                                                    </object>
                                                </child>
                                            </object>
                                        </child>
                                    </object>
                                </child>

                            </object>
                        </child>

                        <child>
                            <object class="GtkBox">
                                <property name="orientation">vertical</property>
                                <property name="spacing">18</property>
                                

                                <child>
                                    <object class="AdwPreferencesGroup" id="interpretation_group">
                                        <child>
                                            <object class="AdwActionRow" id="interpretation_row"/>
                                        </child>
                                    </object>
                                </child>

                                <child>
                                    <object class="AdwPreferencesGroup" id="analysis_group">
                                        <child>
                                            <object class="AdwActionRow" id="analysis_row"/>
                                        </child>
                                    </object>
                                </child>

                                <child>
                                    <object class="AdwPreferencesGroup" id="reliability_group">
                                        <child>
                                            <object class="AdwActionRow" id="reliability_row"/>
                                        </child>
                                    </object>
                                </child>

                            </object>
                        </child>

                    </object>
                </child>

            </object>
        </child>

        <child>
            <object class="GtkButton" id="return_button">
                <property name="halign">center</property>
                <property name="margin-top">24</property>
                <style>
                    <class name="pill"/>
                    <class name="suggested-action"/>
                </style>
            </object>
        </child>

    </object>
</interface>
